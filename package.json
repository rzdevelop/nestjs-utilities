{
  "name": "nestjs-utilities",
  "version": "4.0.0",
  "description": "NestJS Package including abstractions for typeorm, authentication, pagination, configuration and more",
  "author": "Renan Zelaya <renan_zelaya@hotmail.com>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/rzdevelop/nestjs-utilities.git"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' -c .eslintrc.js",
    "test": "jest --config jestconfig.json",
    "test:coverage": "jest --config jestconfig.json  --coverage --coverageReporter=\"json-summary\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "deploy": "npm version patch && npm publish"
  },
  "keywords": [
    "nestjs",
    "typeorm",
    "cqrs"
  ],
  "license": "MIT",
  "devDependencies": {
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "^9.3.10",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.10",
    "@nestjs/cqrs": "^9.0.3",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/terminus": "^9.2.1",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.0",
    "@types/ramda": "^0.28.23",
    "@types/uuid": "^9.0.1",
    "@types/validator": "^13.7.14",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "nestjs-pino": "^3.1.2",
    "pino-http": "^8.3.3",
    "prettier": "^2.8.4",
    "prettier-plugin-sort-imports": "^1.7.1",
    "ramda": "^0.28.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.0",
    "rxjs": "^7.8.0",
    "ts-jest": "^29.0.5",
    "typeorm": "^0.3.12",
    "typescript": "^4.9.5"
  },
  "files": [
    "lib/**/*"
  ],
  "peerDependencies": {
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "^9.3.10",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.10",
    "@nestjs/cqrs": "^9.0.3",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/terminus": "^9.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "nestjs-pino": "^3.1.2",
    "pino-http": "^8.3.3",
    "ramda": "^0.28.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.12"
  }
}
