{
  "name": "nestjs-utilities",
  "version": "3.0.0",
  "description": "NestJS Package including abstractions for typeorm, authentication, pagination, configuration and more",
  "author": "Renan Zelaya <renan_zelaya@hotmail.com>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/rzdevelop/nestjs-utilities.git"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' -c .eslintrc.js",
    "test": "jest --config jestconfig.json",
    "test:coverage": "jest --config jestconfig.json  --coverage --coverageReporter=\"json-summary\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "deploy": "npm version patch && npm publish"
  },
  "keywords": [
    "nestjs",
    "typeorm",
    "cqrs"
  ],
  "license": "MIT",
  "devDependencies": {
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "^9.0.5",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.5",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/terminus": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.6",
    "@types/ramda": "^0.28.15",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.1",
    "jest": "^28.1.3",
    "nestjs-pino": "^3.1.1",
    "pino-http": "^8.1.1",
    "prettier": "^2.7.1",
    "ramda": "^0.28.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "ts-jest": "^28.0.7",
    "typeorm": "^0.3.7",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  },
  "files": [
    "lib/**/*"
  ],
  "peerDependencies": {
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "^9.0.5",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.5",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/terminus": "^9.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "nestjs-pino": "^3.1.1",
    "pino-http": "^8.1.1",
    "ramda": "^0.28.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "typeorm": "^0.3.7"
  }
}
