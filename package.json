{
  "name": "nestjs-utilities",
  "version": "0.1.10",
  "description": "NestJS Package including abstractions for typeorm, authentication, pagination, configuration and more",
  "author": "Renan Zelaya <renan_zelaya@hotmail.com>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/rzdevelop/nestjs-utilities.git"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "deploy": "npm version patch && npm publish"
  },
  "keywords": [
    "nestjs",
    "typeorm",
    "cqrs"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/uuid": "^8.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/cqrs": "^7.0.1",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/terminus": "^7.1.2",
    "class-validator": "^0.13.1",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.0.1",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/cqrs": "^7.0.1",
    "@nestjs/terminus": "^7.1.2",
    "class-validator": "^0.13.1",
    "joi": "^17.4.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.1",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  }
}
